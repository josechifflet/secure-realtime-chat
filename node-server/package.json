        "nodemon": "^2.0.15",
        "onchange": "^7.1.0",
        "prettier": "^2.7.0",
        "source-map-loader": "^0.2.4",
        "ts-loader": "^5.3.1",
        "ts-node": "^6.2.0",
        "tslint": "5.8.0",
        "tslint-config-airbnb": "^5.8.0",
        "typescript": "^4.2.3"
    }
