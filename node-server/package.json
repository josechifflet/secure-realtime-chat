        "compression": "1.7.1",
        "cookie-parser": "1.4.3",
        "cors": "2.8.4",
        "cron": "^1.3.0",
        "dotenv": "^4.0.0",
        "ejs": "^2.6.1",
        "express": "4.16.2",
        "express-custom-responses": "^1.0.0",
        "express-winston": "^2.5.1",
        "helmet": "^5.0.2",
        "http-errors": "^1.6.3",
        "jsonwebtoken": "^8.5.1",
        "lodash": "^4.17.21",
        "moment": "^2.29.3",
        "mongo-seeding": "^3.6.0",
        "mongoose": "^6.3.1",
        "multer": "^1.4.1",
