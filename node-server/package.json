        "@types/cron": "^1.3.0",
        "@types/debug": "0.0.31",
        "@types/ejs": "^2.6.1",
        "@types/express": "4.11.0",
        "@types/helmet": "0.0.42",
        "@types/node": "^8.10.38",
        "@types/socket.io": "^2.1.11",
